{
    "manifest_version": 2,
    "name": "Click It",
    "description": "My first Chrome extension.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Create Pdf google Extension",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "popup.js"
        ]
    },
    "permissions": [
        "activeTab",
        "pageCapture",
        "downloads",
        "tabs",
        "webNavigation",
        "contextMenus",
        "downloads",
        "<all_urls>"
    ],
    "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
   
}